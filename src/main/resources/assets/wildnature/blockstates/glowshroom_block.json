{
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "wildnature:block/glowshroom_block" }
        },
        {   "when": { "east": true },
            "apply": { "model": "wildnature:block/glowshroom_block", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "wildnature:block/glowshroom_block", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "wildnature:block/glowshroom_block", "y": 270, "uvlock": true }
        },
        {   "when": { "up": true },
            "apply": { "model": "wildnature:block/glowshroom_block", "x": 270, "uvlock": true }
        },
        {   "when": { "down": true },
            "apply": { "model": "wildnature:block/glowshroom_block", "x": 90, "uvlock": true }
        },
        {   "when": { "north": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block" }
        },
        {   "when": { "east": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block", "y": 90, "uvlock": false }
        },
        {   "when": { "south": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block", "y": 180, "uvlock": false }
        },
        {   "when": { "west": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block", "y": 270, "uvlock": false }
        },
        {   "when": { "up": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block", "x": 270, "uvlock": false }
        },
        {   "when": { "down": false },
            "apply": { "model": "wildnature:block/glowshroom_inside_block", "x": 90, "uvlock": false }
        }
    ]
}
