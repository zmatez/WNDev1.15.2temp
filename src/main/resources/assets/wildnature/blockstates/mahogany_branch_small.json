{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_up" }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_down" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_side", "y": 270, "uvlock": true }
        },
        {   "when": { "waterlogged": "true" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_none"}
        },
        {   "when": { "waterlogged": "false" },
            "apply": { "model": "wildnature:block/mahogany_branch_small_none"}
        }
    ]
}
